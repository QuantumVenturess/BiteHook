<%= content_for :head do %>
	<meta property="fb:app_id" content="154291521379396" /> 
	<meta property="og:type"   content="bitehook_offline:event" /> 
	<meta property="og:title"  content="<%= @event.name %>" />
	<meta property="og:image"  content="<%= @event.image1 %>" />
	<meta property="og:description" content="<%= @event.info %>" />
	<meta property="og:url"    content="http://bitehook.com<%= event_path(@event) %>" />
	<meta property="og:site_name" content="BiteHook"/>
<% end %>